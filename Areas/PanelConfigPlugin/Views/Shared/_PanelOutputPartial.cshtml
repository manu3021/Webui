<script src="~/Scripts/PanelConfigPlugin/panel-output-model.js"></script>
<script src="~/Scripts/PanelConfigPlugin/panel-output-validation-context.js"></script>
<script src="~/Scripts/PanelConfigPlugin/panel-output-datacontext.js"></script>
<script src="~/Scripts/PanelConfigPlugin/panel-output-uicontext.js"></script>*@